{
    "manifest_version": 2,
    "name": "Page Tweak",
    "options_page": "popup.html",
    "version": "1.0",
    "background": {
        "scripts": [
            "jquery-3.1.1.min.js",
            "background.js"
        ]
    },
    "browser_action": {
        "name": "config page"
    },
    "permissions": [
        "tabs",
        "<all_urls>",
        "http://*/*",
        "https://*/*",
        "contextMenus",
        "storage"
    ]
    /*"icons": {
          "16": "icon-bitty.png"
        },*/
    //"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}